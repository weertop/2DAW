package Clases;

/**
 *  Esta es una subclase de Entradas que hereda sus atributos
 * y metodo al que sobreescribe 
 * @author Alberto Camara, Alexis Calderon
 *
 */

public class EntradaNormal extends Entradas{
	/**
	 * Constructor de subclase EntradaNormal
	 * @param precioEntrada
	 */
	public EntradaNormal(double precioEntrada) {
		super(precioEntrada);
	}
	
	/**
	 * Metodo sobreescrito para esta subclase, getPrecio() devolvera
	 * una a getprecioEntrada() de la clase superior, esta contiene 
	 * el precio
	 * @return getprecioEntrada();
	 */
	public double getPrecio() {
		return super.getprecioEntrada();
	}
}
